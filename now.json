{
  "version": 2,
  "name": "micro-portalen-roles",
  "alias": [
    "roles.portalen.win"
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "builds": [
    { "src": "README.md", "use": "@now/md"},
    { "src": "index.js", "use": "@now/node"}
  ],
  "routes": [
    { "src": "/", "dest": "/README.html" },
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}